  <appSettings>
    <add key="IsTracing" value="true"/>
    <add key="SBTApiKey" value="mjnP64oDbum4WfHavww80xREp1/2IyzJwRJEGUj1RoOX+cMdH0uPw3eB9bnq29G3"/>
    <add key="SBTOrganizationCode" value="7nj24j9"/>
